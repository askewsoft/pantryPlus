{
  "cli": {
    "version": ">= 12.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "environment": "preview",
      "distribution": "internal",
      "node": "22.6.0",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "updates": {
        "channel": "preview"
      }
    },
    "production": {
      "environment": "production",
      "node": "22.6.0",
      "autoIncrement": true,
      "ios": {},
      "updates": {
        "channel": "production"
      }
    },
    "development-simulator": {
      "environment": "development",
      "distribution": "internal",
      "node": "22.6.0",
      "developmentClient": true,
      "ios": {
        "simulator": true
      },
      "updates": {
        "channel": "development"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
