    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^7.0.6",
    "@react-navigation/native": "^7.0.4",
    "@react-navigation/stack": "^7.0.6",
    "aws-amplify": "^6.6.7",
    "date-fns": "^4.1.0",
    "expo": "^51.0.38",
    "expo-application": "^5.9.1",
    "expo-crypto": "~13.0.2",
    "expo-location": "~17.0.1",
    "expo-splash-screen": "^0.27.7",
