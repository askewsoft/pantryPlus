{
  "name": "pantryplus",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 expo start",
    "prebuild": "NODE_NO_WARNINGS=1 expo prebuild --clean  # This will regenerate the native iOS files",
    "ios": "NODE_NO_WARNINGS=1 expo run:ios",
    "android": "NODE_NO_WARNINGS=1 expo run:android",
    "build:sim:ios": "eas build --platform ios --profile preview",
    "build:prod:ios": "eas build --platform ios --profile production",
    "killport": "sudo kill -9 $(sudo lsof -t -i :8081) # Do this if the Metro Bundler fails to start"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.6",
